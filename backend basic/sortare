#include<iostream>

using namespace std;

int main(){
    int i, j, numere, aux;
    
    cout<<"Introduceti numarul de numere ce doriti a fi sortate:";
    cin>>numere;

    int v[numere];

    for(i=0;i<numere;i++)
    cin>>v[i];

    for(i=0;i<numere-1;i++)
    for(j=i+1;j<numere;j++){
        if (v[j]<v[i]){
           aux=v[i];
           v[i]=v[j];
           v[j]=aux;
        }
    }
        cout<<"Vectorul sortat este: "<<endl;
        for(i=0;i<numere;i++)
        cout<<v[i]<<" ";
    
    return 0;
}
